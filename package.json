{
  "name": "webpack-demo1",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dev-server --hotOnly --progress --config ./config/webpack.config.dev.js ",
    "dev:server": "cross-env NODE_ENV=production npm-run-all --parallel build:client-watch build:server-watch ",
    "build:client-watch": "cross-env BUILD_TYPE=client webpack --progress --watch   --config ./config/webpack.config.prod.js",
    "build:server-watch": "cross-env BUILD_TYPE=server webpack --progress --watch   --config ./config/webpack.config.prod.js",
    "build": "cross-env NODE_ENV=production npm-run-all --parallel build:client build:server",
    "build:client": "cross-env BUILD_TYPE=client webpack --progress   --config ./config/webpack.config.prod.js",
    "build:server": "cross-env BUILD_TYPE=server webpack --progress   --config ./config/webpack.config.prod.js",
    "server": "nodemon ./server/index.js --watch config --watch server"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-dynamic-import-node": "^1.2.0",
    "babel-plugin-dynamic-import-webpack": "^1.0.2",
    "babel-plugin-remove-webpack": "^1.1.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "clean-webpack-plugin": "^0.1.17",
    "cross-env": "^5.1.1",
    "css-loader": "^0.28.7",
    "html-webpack-plugin": "^2.30.1",
    "nodemon": "^1.12.5",
    "npm-run-all": "^4.1.2",
    "progress-bar-webpack-plugin": "^1.10.0",
    "style-loader": "^0.19.0",
    "webpack-dev-server": "^2.9.7",
    "webpack-manifest-plugin": "^1.3.2"
  },
  "dependencies": {
    "history": "^4.7.2",
    "koa": "^2.4.1",
    "koa-static": "^4.0.2",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-helmet": "^5.2.0",
    "react-loadable": "^5.3.1",
    "react-redux": "^5.0.6",
    "react-router-config": "^1.0.0-beta.4",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.6",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0"
  }
